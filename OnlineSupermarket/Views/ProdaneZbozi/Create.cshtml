@model OnlineSupermarket.Models.ProdaneZbozi

@{
	ViewData["Title"] = "Создать Новый Проданный Товар";
}

<h2>Создать Новый Проданный Товар</h2>

<h4>ProdaneZbozi</h4>
<hr />
<div class="row">
	<div class="col-md-4">
		<form asp-action="Create">
			<div class="form-group">
				<label asp-for="Pocet" class="control-label"></label>
				<input asp-for="Pocet" class="form-control" />
				<span asp-validation-for="Pocet" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="Prodejnicena" class="control-label"></label>
				<input asp-for="Prodejnicena" class="form-control" />
				<span asp-validation-for="Prodejnicena" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="ZboziIdZbozi" class="control-label"></label>
				<input asp-for="ZboziIdZbozi" class="form-control" />
				<span asp-validation-for="ZboziIdZbozi" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="IdTranzakce" class="control-label"></label>
				<input asp-for="IdTranzakce" class="form-control" />
				<span asp-validation-for="IdTranzakce" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Сохранить" class="btn btn-primary" />
			</div>
		</form>
	</div>
</div>

@section Scripts {
	@await Html.PartialAsync("_ValidationScriptsPartial")
}