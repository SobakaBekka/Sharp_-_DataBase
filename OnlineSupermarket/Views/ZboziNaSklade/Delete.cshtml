@model OnlineSupermarket.Models.ZboziNaSklade

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Zbozi na sklade</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Pocet</dt>
        <dd class="col-sm-10">@Model.Pocet</dd>
        <dt class="col-sm-2">Zbozi ID</dt>
        <dd class="col-sm-10">@Model.ZboziIdZbozi</dd>
        <dt class="col-sm-2">Sklad ID</dt>
        <dd class="col-sm-10">@Model.SkladIdSkladu</dd>
        <dt class="col-sm-2">Adresa ID</dt>
        <dd class="col-sm-10">@Model.AdresaIdAdresy</dd>
        <dt class="col-sm-2">Kategorie ID</dt>
        <dd class="col-sm-10">@Model.KategorieIdKategorii</dd>
        <dt class="col-sm-2">Datum vytvoreni</dt>
        <dd class="col-sm-10">@Model.DatumVytvoreni</dd>
        <dt class="col-sm-2">Datum aktualizace</dt>
        <dd class="col-sm-10">@Model.DatumAktualizace</dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="ZboziIdZbozi" />
        <input type="hidden" asp-for="SkladIdSkladu" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>